    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/body-login.css">
    <title>Login/Criar Conta</title>

</head>

<body>
    <main>
        <div class="alternative">
            <span>OR</span>
        </div>

        <form action="">
            <label for="name">
                <span>Name</span>
                <input type="text" id="name" name="name">
            </label>

            <label for="password">
                <span>Password</span>
                <input type="password" id="password" name="password">
            </label>

            <label for="email">
                <span>E-mail</span>
                <input type="email" id="email" name="email">
            </label>
            <input type="submit" value="Sign Up">

        </form>

    </main>
    <section class="images">
        <img src="cc/mobile.svg" alt="Mobile">
        <div class="circle"></div>
    </section>

    <h1>Create Account</h1>
    <div class="social-media">
        <a href="https://www.google.com.br/%22%3E
                <img src=" cc/google.png" alt="Google">
        </a>
        <a href="https://github.com/gabrielts212%22%3E
                <img src=" cc/github.svg" alt="github">
        </a>
        <a href="https://www.linkedin.com/in/gabriel-nascimento-417b05214/%22%3E
                <img src=" cc/linkedin.png" alt="linkedin">
        </a>
    </div>

